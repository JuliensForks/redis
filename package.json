{
  "name": "@adonisjs/redis",
  "version": "3.2.2",
  "description": "AdonisJS addon for Redis",
  "main": "build/providers/RedisProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src",
    "build/templates"
  ],
  "types": "build/adonis-typings/redis.d.ts",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "docker-compose build && docker-compose run --rm tests",
    "lint": "eslint . --ext=.ts",
    "clean": "del build",
    "copyfiles": "copyfiles \"templates/**/*.txt\" build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile && npm run copyfiles",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-redis.git"
  },
  "keywords": [
    "redis",
    "ioredis"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-redis/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-redis#readme",
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "devDependencies": {
    "@adonisjs/application": "^1.3.12",
    "@adonisjs/core": "^5.0.0-preview-rc-1.2",
    "@adonisjs/fold": "^6.3.5",
    "@adonisjs/mrm-preset": "^2.3.0",
    "@types/node": "^13.13.0",
    "commitizen": "^4.0.4",
    "copyfiles": "^2.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-adonis": "^1.0.9",
    "husky": "^4.2.5",
    "japa": "^3.0.1",
    "mrm": "^2.2.1",
    "np": "^5.2.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.0.0-preview-rc"
  },
  "dependencies": {
    "@poppinss/utils": "^2.2.4",
    "@types/ioredis": "^4.14.9",
    "ioredis": "^4.16.2"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "adonisjs": {
    "templates": {
      "config": [
        {
          "src": "config.txt",
          "dest": "redis"
        }
      ],
      "contracts": [
        {
          "src": "contract.txt",
          "dest": "redis"
        }
      ]
    },
    "types": "@adonisjs/redis",
    "providers": [
      "@adonisjs/redis"
    ],
    "env": {
      "REDIS_CONNECTION": "local",
      "REDIS_HOST": "127.0.0.1",
      "REDIS_PORT": "6379",
      "REDIS_PASSWORD": ""
    }
  }
}
